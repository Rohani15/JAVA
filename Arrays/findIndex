class HelloWorld 
{
    public static void main(String[] args) 
    {
        int A[] = {3,9,7,8,12,6,15,5,4,10};
        int sum = 0; 
        int key = 6; 
        
        for(int i=0; i<A.length; i++)
        {
            if(A[i] == key)
            {
                System.out.println("The Index of the Key is: " + i); 
                System.exit(0);
            }
        }
        //System.out.println(sum); 
    }
}
